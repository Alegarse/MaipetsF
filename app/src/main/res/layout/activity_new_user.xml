<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBack"
    tools:context=".NewUserActivity">

    <ImageView
        android:id="@+id/familyuser"
        android:layout_width="272dp"
        android:layout_height="173dp"
        android:layout_marginTop="68dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/patron" />


    <TextView
        android:id="@+id/familyusertext"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:inputType="textMultiLine"
        android:longClickable="true"
        android:text="@string/familyuser"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="26dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/familyuser"
        app:layout_constraintEnd_toEndOf="@+id/familyuser"
        app:layout_constraintStart_toStartOf="@+id/familyuser"
        app:layout_constraintTop_toTopOf="@+id/familyuser"
        app:layout_constraintVertical_bias="0.065" />

    <ImageView
        android:layout_width="130dp"
        android:layout_height="97dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/familyuser"
        app:layout_constraintEnd_toEndOf="@+id/familyuser"
        app:layout_constraintStart_toStartOf="@+id/familyuser"
        app:layout_constraintTop_toTopOf="@+id/familyuser"
        app:layout_constraintVertical_bias="0.894"
        app:srcCompat="@drawable/familiamascotas" />

    <ImageView
        android:id="@+id/vetuser"
        android:layout_width="272dp"
        android:layout_height="173dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/familyuser"
        app:srcCompat="@drawable/patron" />

    <TextView
        android:id="@+id/vetusertext"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:inputType="textMultiLine"
        android:longClickable="true"
        android:text="@string/vetuser"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="26dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/vetuser"
        app:layout_constraintEnd_toEndOf="@+id/vetuser"
        app:layout_constraintStart_toStartOf="@+id/vetuser"
        app:layout_constraintTop_toTopOf="@+id/vetuser"
        app:layout_constraintVertical_bias="0.094" />

    <ImageView
        android:layout_width="146dp"
        android:layout_height="111dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/vetuser"
        app:layout_constraintEnd_toEndOf="@+id/vetuser"
        app:layout_constraintStart_toStartOf="@+id/vetuser"
        app:layout_constraintTop_toTopOf="@+id/vetuser"
        app:layout_constraintVertical_bias="0.887"
        app:srcCompat="@drawable/veterinarios" />

    <ImageView
        android:id="@+id/anotheruser"
        android:layout_width="272dp"
        android:layout_height="173dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vetuser"
        app:srcCompat="@drawable/patron" />

    <TextView
        android:id="@+id/anotherusertext"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:inputType="textMultiLine"
        android:longClickable="true"
        android:text="@string/anotheruser"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="26dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/anotheruser"
        app:layout_constraintEnd_toEndOf="@+id/anotheruser"
        app:layout_constraintStart_toStartOf="@+id/anotheruser"
        app:layout_constraintTop_toTopOf="@+id/anotheruser"
        app:layout_constraintVertical_bias="0.144" />

    <ImageView
        android:layout_width="145dp"
        android:layout_height="79dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/anotheruser"
        app:layout_constraintEnd_toEndOf="@+id/anotheruser"
        app:layout_constraintStart_toStartOf="@+id/anotheruser"
        app:layout_constraintTop_toTopOf="@+id/anotheruser"
        app:layout_constraintVertical_bias="0.887"
        app:srcCompat="@drawable/anotheruser" />


</androidx.constraintlayout.widget.ConstraintLayout>